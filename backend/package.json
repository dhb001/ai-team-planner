{
  "name": "ai-team-planner-backend",
  "version": "1.0.0",
  "description": "AI Team Planner Backend API",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "db:schema": "mysql -u root -p ai_team_planner < src/schema.sql",
    "db:seed": "mysql -u root -p ai_team_planner < src/seeds.sql",
    "db:setup": "npm run db:schema && npm run db:seed"
  },
  "keywords": [
    "ai",
    "planning",
    "team",
    "nodejs",
    "openrouter"
  ],
  "author": "AI Team Planner",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "mysql2": "^3.6.0",
    "dotenv": "^16.3.1",
    "dayjs": "^1.11.9"
  },
  "devDependencies": {
    "typescript": "^5.2.2",
    "ts-node": "^10.9.1",
    "@types/node": "^20.5.0",
    "@types/express": "^4.17.17",
    "@types/cors": "^2.8.13"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
